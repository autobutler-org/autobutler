{
  "name": "autobutler",
  "version": "1.0.0",
  "type": "module",
  "description": "## Important Links",
  "scripts": {
    "test/e2e": "npx playwright test",
    "test/e2e/report": "npx playwright show-report",
    "test/e2e/install": "npx playwright install",
    "lint:js": "eslint internal/server/public/scripts/**/*.js && prettier --check internal/server/public/scripts/**/*.js",
    "lint:ts": "eslint tests/e2e/**/*.ts && prettier --check tests/e2e/**/*.ts",
    "lint:css": "prettier --check internal/server/public/styles/**/*.css",
    "lint:yaml": "yamllint '**/*.{yaml,yml}'",
    "format:js": "eslint --fix internal/server/public/scripts/**/*.js && prettier --write internal/server/public/scripts/**/*.js",
    "format:ts": "eslint --fix tests/e2e/**/*.ts && prettier --write tests/e2e/**/*.ts",
    "format:css": "prettier --write internal/server/public/styles/**/*.css",
    "check-updates": "ncu -u"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "npm-check-updates": "^19.1.2",
    "prettier": "^3.6.2",
    "yaml-lint": "^1.7.0"
  }
}

  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug server",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "serve",
      ],
      "envFile": "${workspaceFolder}/.env",
      "preLaunchTask": "make: generate"
    },
    {
      "name": "Debug running process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 115725,
    }
  ]
}

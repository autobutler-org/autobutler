package views

import (
	"autobutler/internal/server/ui/components/body"
	"autobutler/internal/server/ui/components/header"
	"autobutler/internal/server/ui/components/hero"
	"autobutler/internal/server/ui/types"
)

templ Home(pageState types.PageState) {
	{{ pageState.CurrentPageName = types.PageHome }}
	<!DOCTYPE html>
	<html lang="en">
		@header.Component()
		@body.Component(pageState) {
			@hero.Component()
		}
	</html>
}

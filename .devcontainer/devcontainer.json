{
  "name": "Devcontainer",
  "dockerComposeFile": [
    "../devcontainer.compose.yml"
  ],
  "service": "code",
  "workspaceFolder": "/code",
  "shutdownAction": "stopCompose",
  "containerEnv": {
    "SHELL": "/usr/bin/zsh"
  },
  "postCreateCommand": "make setup",
  "forwardPorts": [
    // Live reload server
    7331,
    // Normal server port
    8080
  ],
  "customizations": {
    "vscode": {}
  }
}